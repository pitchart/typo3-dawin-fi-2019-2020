    "description": "Ceci est une description.",
    "authors": [
        {
            "name": "Julien Vitte",
            "role": "Developer"
        }
    ],
    "require": {
        "typo3/cms-core": "^8.7.1"
    },
    "autoload": {
        "psr-4": {
